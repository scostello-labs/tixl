    {
      "InputId": "20c56912-81dc-44f2-b44a-91febe627f75"/*MinSpread*/,
      "Position": {
        "X": 394.841,
        "Y": 467.03152
      },
      "Description": "A minimum \"noise cushion\" so perfectly flat areas don’t become hypersensitive. Increase it if you see sparkly false positives in quiet regions.",
      "Min": 0.0,
      "Max": 1.0,
      "Scale": 0.001,
      "ClampMin": true,
      "ClampMax": true
    },
    {
